<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.interactivitydemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TicTacToeActivity"></activity>
        <activity android:name=".PictureViewer" />
        <activity
            android:name=".TriangularNumbers"
            android:label="@string/title_triangular_numbers" />
        <activity
            android:name=".GuessANumber"
            android:label="@string/title_guess_number"
            android:noHistory="true" />
        <activity
            android:name=".CurrencyConverter"
            android:label="@string/title_currency_converter"
            android:noHistory="true" />
        <activity
            android:name=".ImageActivity"
            android:noHistory="true" />
        <activity
            android:name=".InterActivityDemo"
            android:noHistory="true" />
        <activity
            android:name=".JavaTest"
            android:label="@string/title_java_test"
            android:noHistory="true" />
        <activity
            android:name=".AnimationBart"
            android:label="@string/title_animation_bart"
            android:noHistory="true" />
        <activity
            android:name=".MyNavActivity"
            android:label="@string/title_activity_topics_covered_menu"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TopicsCovered"
            android:noHistory="true" />
        <activity
            android:name=".MainActivity"
            android:noHistory="true" />
    </application>

</manifest>